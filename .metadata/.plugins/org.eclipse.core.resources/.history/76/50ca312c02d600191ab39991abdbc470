package ua.khpi.oop.goriansckiy02;

public class Main {
	public static void main(String []args) {
//		int x=4;
//		int res = 4%10+1;
//		System.out.println(res);
		
			int n = 25;
	        int i;
	        boolean isComposite = false;
	        for (i = 2; i < n; i++) {
	            if (n % i == 0) {
	                isComposite = true;
	                break;
	            }
	        }
	        if (isComposite) {
	            System.out.println("число составное ");
	        } else {
	            System.out.println("число  простое ");
	        }
	}
}
